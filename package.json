{
  "name": "cookie-toss",
  "version": "2.0.3",
  "description": "A package that makes sharing local data across domains a thing of ease.",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "scriptTag": "dist/index.js",
  "testScriptTag": "dist/test-app.js",
  "scripts": {
    "dev-iframe": "parcel ./examples/iframe.html --port 1111",
    "dev-app": "parcel ./examples/app.html --port 2222",
    "build": "rm -rf lib dist && rollup -c config.rollup.js",
    "deploy": "npm run build && npm publish --access public",
    "test": "jest"
  },
  "author": "john.mealy@gmail.com",
  "license": "ISC",
  "keywords": [
    "cookie",
    "cookies",
    "browser",
    "iframe",
    "iframes",
    "cross-origin",
    "cross-domain",
    "cross-site",
    "storage",
    "localStorage",
    "local-storage",
    "data-sharing",
    "data-access",
    "user-data"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/JohnMealy23/cookie-toss"
  },
  "dependencies": {
    "@types/node": "^9.3.0",
    "@types/url-parse": "^1.4.3",
    "types": "^0.1.1",
    "typescript": "^3.6.3",
    "url-parse": "^1.4.7",
    "utils": "^0.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@types/jest": "^23.3.2",
    "jest": "^25.0.0",
    "rollup": "^2.3.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-visualizer": "^4.0.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.8.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1"
  }
